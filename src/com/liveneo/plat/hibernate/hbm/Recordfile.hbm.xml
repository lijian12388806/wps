<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.liveneo.plat.hibernate.dao">
	<class
		name="Recordfile"
		table="recordfile"
	>
		<meta attribute="sync-DAO">true</meta>
		<composite-id name="Id" class="RecordfilePK">
			<key-property
				name="ContactID"
				column="ContactID"
				type="string"
			/>
			<key-property
				name="StartTime"
				column="StartTime"
				type="string"
			/>
			<key-property
				name="DeviceDN"
				column="DeviceDN"
				type="string"
			/>
		</composite-id>

		<property
			name="AgentLoginID"
			column="AgentLoginID"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="DeviceType"
			column="DeviceType"
			type="big_decimal"
			not-null="false"
			length="2"
		/>
		<property
			name="CallDirection"
			column="CallDirection"
			type="big_decimal"
			not-null="false"
			length="2"
		/>
		<property
			name="CallType"
			column="CallType"
			type="big_decimal"
			not-null="false"
			length="2"
		/>
		<property
			name="OriANI"
			column="OriANI"
			type="string"
			not-null="true"
			length="64"
		/>
		<property
			name="OriDNIS"
			column="OriDNIS"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="TimeLength"
			column="TimeLength"
			type="big_decimal"
			not-null="false"
			length="32"
		/>
		<property
			name="FileName"
			column="FileName"
			type="string"
			not-null="true"
			length="256"
		/>
		<property
			name="CallID"
			column="CallID"
			type="big_decimal"
			not-null="false"
			length="32"
		/>
		<property
			name="OldCallID"
			column="OldCallID"
			type="big_decimal"
			not-null="false"
			length="32"
		/>
		<property
			name="SecOldCallID"
			column="SecOldCallID"
			type="big_decimal"
			not-null="false"
			length="32"
		/>
		<property
			name="CallingParty"
			column="CallingParty"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="CalledParty"
			column="CalledParty"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="OtherParty"
			column="OtherParty"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="ThirdParty"
			column="ThirdParty"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="UserField"
			column="UserField"
			type="string"
			not-null="false"
			length="64"
		/>
		<property
			name="Uploadadress"
			column="uploadadress"
			type="string"
			not-null="false"
			length="64"
		/>


	</class>	
</hibernate-mapping>